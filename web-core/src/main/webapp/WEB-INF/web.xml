<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
   <servlet-name>OraclesqlServlet</servlet-name>
   <servlet-class>com.servlet.demo.OraclesqlServlet</servlet-class>
   <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>OraclesqlServlet</servlet-name>
    <url-pattern>/oracle-connector</url-pattern>
  </servlet-mapping>
</web-app>
